{
  "name": "@antv/g-site",
  "version": "1.2.3",
  "private": true,
  "description": "G sites deployed on gh-pages",
  "keywords": [
    "g",
    "antv",
    "site"
  ],
  "homepage": "https://github.com/antvis/g#readme",
  "bugs": {
    "url": "https://github.com/antvis/g/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antvis/g.git"
  },
  "license": "ISC",
  "author": "https://github.com/orgs/antvis/people",
  "files": [
    "package.json",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "site:build": "npm run site:clean && GATSBY=true gatsby build --prefix-paths && cp ../../spec/api.html public",
    "site:clean": "gatsby clean",
    "site:copy-mirror-action": "mkdir -p public/.github/workflows && cp .github/workflows/mirror.yml public/.github/workflows",
    "site:deploy": "npm run site:build && npm run site:copy-mirror-action && gh-pages -d public",
    "site:develop": "GATSBY=true gatsby develop --open",
    "start": "npm run site:clean && npm run site:develop"
  },
  "dependencies": {
    "@antv/g6": "^4.5.2",
    "@antv/gatsby-theme-antv": "^1.1.15",
    "@antv/webgpu-graph": "^1.0.0",
    "@naoak/workerize-transferable": "^0.1.0",
    "@types/gl-matrix": "^2.4.5",
    "@types/jest": "^25.0.0",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "d3": "^7.3.0",
    "d3-force-3d": "^3.0.3",
    "d3-svg-annotation": "^2.5.0",
    "gatsby": "^2.24.40",
    "gatsby-plugin-workerize-loader": "^1.5.0",
    "gh-pages": "^3.2.3",
    "hammer-touchemulator": "^0.0.2",
    "hammerjs": "^2.0.8",
    "interactjs": "^1.10.11",
    "lil-gui": "^0.16.0",
    "react": "^16.0.2",
    "react-dom": "^16.0.2",
    "react-i18next": "^11.1.0",
    "react-split-pane": "^0.1.92",
    "simplex-noise": "^3.0.0",
    "stats.js": "^0.17.0",
    "typedoc": "^0.17.6",
    "typedoc-plugin-markdown": "^2.2.11",
    "webfontloader": "1.6.28"
  }
}
