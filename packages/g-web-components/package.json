    "build": "npm run clean && run-p build:*",
    "sync": "tnpm sync"
  },
  "dependencies": {
    "@antv/g-canvas": "workspace:*",
    "@antv/g-lite": "workspace:*",
    "@antv/g-webgl": "workspace:*"
  },
  "devDependencies": {
    "@types/gl-matrix": "^2.4.5",
    "@types/jest": "^25.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
