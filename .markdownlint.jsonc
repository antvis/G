// https://github.com/DavidAnson/markdownlint/blob/main/schema/.markdownlint.jsonc
{
  "default": true,
  "no-inline-html": false,
  "line-length": false,
  "single-h1": false,
  "first-line-h1": false,
  "no-empty-links": false,
  "no-duplicate-heading": false,
  "link-fragments": false,
  "code-block-style": false,
  "MD003": {
    "style": "atx",
  },
  "MD040": false,
  "MD036": false,
  "MD045": false,
  "front-matter": {
    "type": "yaml",
  },
}
